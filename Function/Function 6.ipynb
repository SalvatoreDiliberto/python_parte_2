   ],
   "source": [
    "def fib(n):\n",
    "    if n<=1:\n",
    "        f=1\n",
    "    else:\n",
    "        f=fib(n-1)+fib(n-2)\n",
    "    return f\n",
    "\n",
    "for x in range(5):\n",
    "    print(fib(x))"
   ]
  }
